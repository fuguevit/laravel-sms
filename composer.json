{
  "name": "fuguevit/laravel-sms",
  "type": "library",
  "description": "Laravel SMS Library",
  "license": "MIT",
  "homepage": "http://www.fuguevit.com",
  "keywords": ["sms","laravel"],
  "authors": [
    {
      "name": "Frank Wang",
      "email": "wangxuan@188.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/support": "~5.2",
    "illuminate/config": "~5.2",
    "nesbot/carbon": "^1.21",
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Fuguevit\\Sms\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fuguevit\\Sms\\Tests\\": "tests/"
    }
  },
  "extra": {
    "component": "package",
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
